<html>
<%- include ../head%>
<body>
<%- include ../header%>
<h1><%= title?title:"Home"%></h1>
<%events.forEach(event => {%>
    <h2><a href="/event/<%=event.id%>"><%= event.name%></a></h2>
    <p><%= event.time%></p>
    <p><%=event.location%></p>
    <%if(user && user.id == event.userId){%>
        <p> editable </p>
    <%}%>
<%})%>
</body>
</html>